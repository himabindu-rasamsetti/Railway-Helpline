					<header>
						<h2>Register as Passenger</h2>
				
					</header>

							
					<div class="row">
						<div class="12u">

							<!-- Form -->
								<section class="box">
									<h3>Registration Form</h3>
									<form method="post" action="register.php">
										<div class="row uniform 50%">
											<div class="6u 12u(mobilep)">
												<input type="text" name="fname" id="fname" value="" placeholder="First Name" required = " "/>
											</div>
											<div class="6u 12u(mobilep)">
												<input type="email" name="email" id="email" value="" placeholder="Email id" required = " "/>
											</div>
											<div class="6u 12u(mobilep)">
												<input type="password" name="password" id="password" value="" placeholder="Password" required = " " />
											</div>
											<div class="6u 12u(mobilep)">
												<input type="text" name="mobile" id="mobile" value="" placeholder="Mobile Number" required = " " />
											</div>
											<div class="6u 12u(mobilep)">
												<label> Data of Journey </label>
												<input type="date" name="date" id="date" value="" placeholder="Data of Journey" required = " " />
											</div>
											<div class="6u 12u(mobilep)">
												<input type="text" name="trainno" id="trainno" value="" placeholder="Train Number" required = " " />
											</div>
											<div class="12u">
												<div class="select-wrapper">
													<select name="type" id="projecttype">
														<option value="">- Category -</option>
														<option value="farmer">Doctor</option>
														<option value="delear">Police</option>
														<option value="customer">Army</option>
														<option value="customer">Fireman</option>
														<option value="customer">Legal Department</option>
													</select>
												</div>
											</div>
										</div>
										<div class="row uniform 50%">
																					<div class="row uniform">
											<div class="12u">
												<ul class="actions">
													<li><input type="submit" class="button special fit" value="Register " /></li>

													
												</ul>
											</div>
										</div>
									</form>
									
								</section>

